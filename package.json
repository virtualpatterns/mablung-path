{
  "author": "virtualpatterns.com <code@virtualpatterns.com> (http://www.virtualpatterns.com)",
  "ava": {
    "failFast": true,
    "files": [
      "release/**/test/**/*.test.*"
    ],
    "nodeArguments": [
      "--no-warnings",
      "--unhandled-rejections=strict"
    ],
    "require": [
      "@virtualpatterns/mablung-source-map-support/install"
    ],
    "verbose": true
  },
  "babel": {
    "presets": [
      [
        "@virtualpatterns/babel-preset-mablung-makefile-environment",
        {
          "header": {
            "exclude": [
              "source/test"
            ]
          }
        }
      ]
    ]
  },
  "c8": {
    "all": true,
    "exclude": [
      "release/header",
      "release/sandbox"
    ],
    "reporter": [
      "html"
    ],
    "src": "release"
  },
  "description": "TBD",
  "eslintConfig": {
    "extends": "@virtualpatterns/eslint-config-mablung-makefile-environment"
  },
  "exports": {
    ".": "./release/index.js",
    "./package.json": "./package.json"
  },
  "files": [
    "release/library",
    "release/index.js"
  ],
  "license": "GPL-3.0+",
  "name": "@virtualpatterns/mablung-path",
  "repository": {
    "type": "git",
    "url": "https://github.com/virtualpatterns/mablung-path"
  },
  "type": "module",
  "version": "0.0.2-22",
  "devDependencies": {
    "@virtualpatterns/babel-preset-mablung-makefile-environment": "0.0.25",
    "@virtualpatterns/eslint-config-mablung-makefile-environment": "0.0.25",
    "@virtualpatterns/mablung-check-dependency": "0.0.25",
    "@virtualpatterns/mablung-makefile": "0.0.25",
    "@virtualpatterns/mablung-makefile-environment": "0.0.25",
    "@virtualpatterns/mablung-source-map-support": "0.0.25",
    "ava": "^3.15.0"
  }
}
